{
  "name": "neilmcgibbon/php-open-rail-data",
  "description": "A PHP libary for processing API events from the Open Rail Data initiative",
  "license": "MIT",
  "require": {
    "php": ">=5.5.0",
    "symfony/event-dispatcher": "2.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.*"
  },
  "autoload": {
    "psr-4": {
      "OpenRailData\\": "src",
      "OpenRailDataExamples\\": "examples"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
